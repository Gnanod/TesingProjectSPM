<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.chart.BarChart?>
<?import javafx.scene.chart.CategoryAxis?>
<?import javafx.scene.chart.NumberAxis?>
<?import javafx.scene.chart.PieChart?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="867.0" prefWidth="1363.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="main.controller.Dashboard.DashboardController">
   <children>
      <BarChart fx:id="subjectChart" layoutX="778.0" layoutY="460.0" prefHeight="379.0" prefWidth="596.0" style="-fx-border-color: #800080;" title="NUMBER OF SUBJECTS FOR SEMESTERS">
        <xAxis>
          <CategoryAxis label="Year and Semester" side="BOTTOM" fx:id="x" />
        </xAxis>
        <yAxis>
          <NumberAxis fx:id="y" label="No of Subjects" side="LEFT" />
        </yAxis>
      </BarChart>
      <BarChart fx:id="EmployeeChart" layoutX="127.0" layoutY="19.0" prefHeight="379.0" prefWidth="596.0" style="-fx-border-color: #800080;" title="NUMBER OF EMPLOYEES FOR FACULTIES">
        <xAxis>
          <CategoryAxis fx:id="yEmp" label="Faculty" side="BOTTOM" />
        </xAxis>
        <yAxis>
          <NumberAxis label="No of Employees" side="LEFT" fx:id="xEmp" />
        </yAxis>
      </BarChart>
      <PieChart fx:id="buildingPiechart" layoutX="778.0" layoutY="19.0" prefHeight="379.0" prefWidth="596.0" style="-fx-border-color: #800080;" title="BUILDINGS OF EACH CENTERS" />
      <PieChart fx:id="desPiechart" layoutX="127.0" layoutY="460.0" prefHeight="379.0" prefWidth="596.0" style="-fx-border-color: #800080;" title="DESIGNATIONS" />
   </children>
</AnchorPane>
